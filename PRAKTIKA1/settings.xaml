<Window x:Class="PRAKTIKA1.settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRAKTIKA1"
        mc:Ignorable="d"
        Title="settings" Height="1024" Width="1440">
    <Window.Background>
        <ImageBrush ImageSource="/1440.1024.png"/>
    </Window.Background>
    <Grid Margin="0,100,0,-16" Width="1440" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="103*"/>
            <ColumnDefinition Width="617*"/>
        </Grid.ColumnDefinitions>
        <Rectangle HorizontalAlignment="Left" Height="80" Stroke="Black" VerticalAlignment="Top" Width="250" Grid.ColumnSpan="2"/>
        <Rectangle HorizontalAlignment="Left" Height="935" Margin="0,79,0,0" Stroke="Black" VerticalAlignment="Top" Width="250" Grid.ColumnSpan="2"/>
        <TextBlock HorizontalAlignment="Left" Margin="46,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="32"><Run Language="ru-ru" Text="Настройки"/></TextBlock>
        <Grid Margin="78,79,10,674" Background="#FFF3F3F3" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0*"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <ComboBox Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top" Width="120"/>
            <ComboBox Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top" Width="120"/>
            <TextBlock Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18"><Run Language="ru-ru" Text="Страна"/></TextBlock>
            <TextBlock Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18"><Run Language="ru-ru" Text="Валюта"/></TextBlock>
        </Grid>
        <TextBlock HorizontalAlignment="Left" Margin="78,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="28" Height="42" Grid.Column="1"><Run Language="ru-ru" Text="Региональные"/><LineBreak/><Run Language="ru-ru"/></TextBlock>
        <Border HorizontalAlignment="Left" Margin="25,98,0,0" VerticalAlignment="Top" Width="200" Height="40" CornerRadius="20" Background="#FFFFAF00" Grid.ColumnSpan="2">
            <Button Content="Button" BorderBrush="{x:Null}" Background="Transparent" Height="40" />
        </Border>
        <Button x:Name="Перелётики" Content="Перелётики" HorizontalAlignment="Left" Margin="112,-90,0,0" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" FontSize="20" Click="Перелётики_Click" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
